<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key ="log4net.Internal.Debug" value="true" />
  </appSettings>
  
  <configSections>
  </configSections>
  
  <connectionStrings>
      <add name="RatEngine.Properties.Settings.ConnString" 
           connectionString="Data Source=DEVELOPER-PC\SQLEXPRESS;Initial Catalog=Rat;Integrated Security=True"
           providerName="System.Data.SqlClient" />
  </connectionStrings>

  <system.diagnostics>
    <trace autoflush="true">
      <listeners>
        <add name="textWriterTraceListener"
             type="System.Diagnostics.TextWriterTraceListener"
             initializeData="c:/users/developer/desktop/log4net.txt" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>